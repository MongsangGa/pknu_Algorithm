int main() {
	fastio;
	int n, m, ans = 0;
	cin >> m >> n;
	vector<uni> v(m, uni(n));
	for (int i = 0; i < m; i++) {
		for (int j = 0; j < n; j++) {
			cin >> v[i].p[j].first;
			v[i].p[j].second = j;
		}
		sort(v[i].p.begin(), v[i].p.end(), compare);
	}


	return 0;
}
