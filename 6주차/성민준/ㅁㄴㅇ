#include <bits/stdc++.h>
using namespace std;
#define fastio ios_base::sync_with_stdio(0);cin.tie(0)

class uni {
public:
	int visit = 0;
	vector<pair<int, int>> p; // 행성 크기, 인덱스
	uni(int n) :p(n) {}
};

bool compare(pair<int, int>& a, pair<int, int>& b) {
	if (a.first != b.first) return a.first < b.first;
	return a.second < b.second;
}

